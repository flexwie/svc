{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "db:push": "echo $DATABASE_URL && prisma db push",
    "build": "run-s build:*",
    "dev": "run-p dev:*",
    "start": "remix-serve build",
    "typecheck": "tsc",
    "build:remix": "cross-env NODE_ENV=production remix build",
    "build:worker": "esbuild ./app/entry.worker.ts --outfile=./public/entry.worker.js --minify --bundle --format=esm --define:process.env.NODE_ENV='\"production\"'",
    "dev:remix": "cross-env NODE_ENV=development remix dev",
    "dev:worker": "esbuild ./app/entry.worker.ts --outfile=./public/entry.worker.js --bundle --format=esm --define:process.env.NODE_ENV='\"development\"' --watch",
    "test": "jest"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.17",
    "@prisma/client": "^4.13.0",
    "@remix-run/css-bundle": "^1.16.0",
    "@remix-run/node": "^1.16.0",
    "@remix-run/react": "^1.16.0",
    "@remix-run/serve": "^1.16.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "draft-js": "^0.11.7",
    "draftjs-md-converter": "^1.5.2",
    "framer-motion": "^10.12.8",
    "i": "^0.3.7",
    "isbot": "^3.6.8",
    "luxon": "^3.3.0",
    "node-persist": "^3.1.0",
    "npm": "^9.6.6",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.7",
    "remix-auth": "^3.4.0",
    "remix-auth-microsoft": "^2.0.0",
    "remix-auth-oauth2": "^1.6.0",
    "remix-auth-webauthn": "^0.0.5",
    "web-push": "^3.4.5"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.16.0",
    "@remix-run/eslint-config": "^1.16.0",
    "@types/draft-js": "^0.11.10",
    "@types/jest": "^29.5.1",
    "@types/luxon": "^3.3.0",
    "@types/node-persist": "^3.1.2",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "eslint": "^8.38.0",
    "jest": "^29.5.0",
    "prisma": "^4.13.0",
    "tailwindcss": "^3.3.2",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=16"
  }
